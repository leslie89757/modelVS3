# ModelVS3 开发环境配置

# ===== 基础配置 =====
APP_NAME=ModelVS3-Dev
ENVIRONMENT=development
SECRET_KEY=dev-secret-key-change-in-production
DEBUG=true

# ===== 开发环境端口配置 =====
DEV_FRONTEND_PORT=3004
DEV_API_PORT=8001
DEV_DB_PORT=5433
DEV_REDIS_PORT=6380
DEV_PROMETHEUS_PORT=8091
DEV_GRAFANA_PORT=3002

# ===== 数据库配置 =====
DATABASE_URL=postgresql://postgres:password@localhost:5433/modelvs3_dev

# ===== Redis 配置 =====
REDIS_URL=redis://localhost:6380/0

# ===== LLM API 密钥 =====
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key
GOOGLE_API_KEY=your-google-api-key

# ===== 安全配置 =====
JWT_SECRET_KEY=dev-jwt-secret-key
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# CORS 配置
CORS_ORIGINS=http://localhost:3004,http://127.0.0.1:3004

# ===== 前端配置 =====
VITE_API_URL=http://localhost:8001

# ===== 监控配置 =====
PROMETHEUS_PORT=8091
GRAFANA_PORT=3002
GF_SECURITY_ADMIN_PASSWORD=dev-admin

# ===== 开发专用配置 =====
LOG_LEVEL=DEBUG
ENABLE_DOCS=true
ENABLE_RELOAD=true 